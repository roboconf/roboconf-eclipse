   </copyright>

   <license url="http://www.apache.org/licenses/LICENSE-2.0">
      Roboconf Eclipse Tooling is licensed under the terms of the Apache License v2.
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.sirius"/>
      <import plugin="net.roboconf.core"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="net.roboconf.eclipse.plugin"/>
      <import plugin="org.eclipse.sirius.ui"/>
      <import plugin="org.eclipse.sirius.ext.base"/>
      <import plugin="org.eclipse.emf.transaction"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
   </requires>

   <plugin
         id="net.roboconf.eclipse.modeler"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.roboconf.eclipse.emf.models"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.roboconf.eclipse.emf.models.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
