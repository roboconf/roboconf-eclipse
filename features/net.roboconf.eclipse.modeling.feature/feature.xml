<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.roboconf.eclipse.modeling.feature"
      label="Roboconf Eclipse Modeling Feature"
      version="0.7.0.qualifier">

   <description url="http://roboconf.net">
      These plug-ins provide modeling tools for Roboconf.
   </description>

   <copyright>
